{
  "name": "markeye",
  "productName": "Markeye",
  "version": "1.0.2",
  "description": "A simple markdown viewer",
  "license": "MIT",
  "repository": "vesparny/markeye",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --out=dist --asar --overwrite --platform=darwin --icon icon.icns"
  },
  "dependencies": {
    "electron-debug": "^3.0.0",
    "electron-window": "^0.8.1",
    "electron-window-state": "^5.0.3",
    "front-matter": "^3.0.2",
    "github-markdown-css": "^3.0.1",
    "highlight.js": "^9.15.6",
    "htm": "^2.1.1",
    "marked": "^0.6.2",
    "mermaid": "^9.1.2",
    "preact": "^8.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "devtron": "^1.4.0",
    "electron": "^5.0.4",
    "electron-packager": "^13.1.1",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.18.2",
    "standard": "^12.0.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ]
  }
}
